<section>

    <div class="grid grid-cols-2 max-w-[1000px] mx-auto gap-y-10">
            <iframe
                src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d1769.7900944780706!2d-58.8341173!3d-27.4823262!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x94456db5773b601d%3A0x8b8e6bd174905021!2sDistribuidora%20Corrientes!5e0!3m2!1ses-419!2sar!4v1705166693503!5m2!1ses-419!2sar"
                class="col-span-2 lg:col-span-1 content-center w-full h-full max-w-lg mx-auto border-solid border-[1px] border-black"
                
                allowfullscreen=""
                loading="lazy"
                referrerpolicy="no-referrer-when-downgrade"></iframe>
         <!--     <img
                class="w-full h-auto max-w-lg mx-auto border-solid border-[1px] border-black"
                src="/mapa.png"
                alt="image description"
            /> -->
        

        <div id="contacto" class="col-span-2 lg:col-span-1 w-full px-4 bg-white">
            <form class="max-w-md mx-auto w-full">
                <div class="relative z-0 w-full mb-5 group">
                    <input
                        type="email"
                        name="floating_email"
                        id="floating_email"
                        class="block py-2.5 px-0 w-full text-sm text-[#10476a] bg-transparent border-0 border-b-2 border-gray-300 appearance-none focus:outline-none focus:ring-0 focus:border-blue-600 peer"
                        placeholder=" "
                        required
                    />
                    <label
                        for="floating_email"
                        class="peer-focus:font-medium absolute text-sm text-black duration-300 transform -translate-y-6 scale-75 top-3 -z-10 origin-[0] peer-focus:start-0 rtl:peer-focus:translate-x-1/4 rtl:peer-focus:left-auto peer-focus:text-blue-600 peer-placeholder-shown:scale-100 peer-placeholder-shown:translate-y-0 peer-focus:scale-75 peer-focus:-translate-y-6"
                        >Correo electr√≥nico</label
                    >
                </div>
                <div class="relative z-0 w-full mb-5 group">
                    <input
                        type="text"
                        name="floating_name"
                        id="floating_name"
                        class="block py-2.5 px-0 w-full text-sm text-gray-900 bg-transparent border-0 border-b-2 border-gray-300 appearance-none focus:outline-none focus:ring-0 focus:border-blue-600 peer"
                        placeholder=" "
                        required
                    />
                    <label
                        for="floating_name"
                        class="peer-focus:font-medium absolute text-sm text-black duration-300 transform -translate-y-6 scale-75 top-3 -z-10 origin-[0] peer-focus:start-0 rtl:peer-focus:translate-x-1/4 peer-focus:text-blue-600 peer-placeholder-shown:scale-100 peer-placeholder-shown:translate-y-0 peer-focus:scale-75 peer-focus:-translate-y-6"
                        >Nombre</label
                    >
                </div>
                <div class="relative z-0 w-full mb-5 group">
                    <textarea
                        name="message"
                        rows="4"
                        id="floating_message"
                        class="resize-none multi block py-2.5 px-0 w-full text-sm text-gray-900 bg-transparent border-0 border-b-2 border-gray-300 appearance-none focus:outline-none focus:ring-0 focus:border-blue-600 peer"
                        placeholder=" "
                        required></textarea>
                    <label
                        for="floating_message"
                        class="peer-focus:font-medium absolute text-sm text-black duration-300 transform -translate-y-6 scale-75 top-3 -z-10 origin-[0] peer-focus:start-0 rtl:peer-focus:translate-x-1/4 peer-focus:text-blue-600 peer-placeholder-shown:scale-100 peer-placeholder-shown:translate-y-0 peer-focus:scale-75 peer-focus:-translate-y-6"
                        >Mensaje</label
                    >
                </div>
                <button
                    type="submit"
                    class="text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm w-full sm:w-auto px-5 py-2.5 text-center"
                    >Enviar</button
                >
            </form>
        </div>
    </div>
</section>
